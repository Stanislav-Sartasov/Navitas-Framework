<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBatteryAmperage" app:layout_constraintStart_toStartOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            android:textSize="16sp"
            android:textAlignment="viewStart"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textViewBatteryVoltage" android:textColor="#000000"/>
    <TextView
            android:text="TEMPERATURE:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView20" app:layout_constraintStart_toStartOf="@+id/guideline1"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:textAlignment="viewStart"
            android:textSize="16sp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textView19" android:textColor="#000000"/>
    <TextView
            android:text="AMPERAGE:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView19" app:layout_constraintStart_toStartOf="@+id/guideline1"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:textAlignment="viewStart"
            android:textSize="16sp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textView18" android:textColor="#000000"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBatteryTemperature" app:layout_constraintStart_toStartOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            android:textSize="16sp"
            android:textAlignment="viewStart"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textViewBatteryAmperage" android:textColor="#000000"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBatteryVoltage" app:layout_constraintStart_toStartOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            android:textSize="16sp"
            android:textAlignment="viewStart"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textViewBatteryLevel" android:textColor="#000000"/>
    <TextView
            android:text="VOLTAGE:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView18" app:layout_constraintStart_toStartOf="@+id/guideline1"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:textAlignment="viewStart"
            android:textSize="16sp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textView16" android:textColor="#000000"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBatteryLevel" app:layout_constraintStart_toStartOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            android:textSize="16sp"
            android:textAlignment="viewStart"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textViewBatteryPlugged" android:textColor="#000000"/>
    <TextView
            android:text="LEVEL:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView16" app:layout_constraintStart_toStartOf="@+id/guideline1"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:textAlignment="viewStart"
            android:textSize="16sp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textView12" android:textColor="#000000"/>
    <TextView
            android:text="STATUS:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            android:textAlignment="viewStart"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/guideline1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="@+id/guideline9" android:textColor="#000000"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline1"
                                                android:orientation="vertical"
                                                app:layout_constraintStart_toStartOf="parent"
                                                android:layout_marginStart="21dp"
                                                app:layout_constraintTop_toTopOf="parent"
                                                app:layout_constraintGuide_percent="0.2"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2"
                                                android:orientation="vertical"
                                                android:layout_marginEnd="41dp"
                                                app:layout_constraintEnd_toEndOf="parent"
                                                app:layout_constraintGuide_percent="0.8"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="0dp" android:layout_height="0dp"
                                                android:id="@+id/guideline7"
                                                android:orientation="vertical" app:layout_constraintTop_toTopOf="parent"
                                                app:layout_constraintBottom_toBottomOf="parent"
                                                app:layout_constraintGuide_percent="0.50121653"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBatteryStatus" app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintStart_toStartOf="@+id/guideline7"
            android:textAlignment="viewStart" android:textSize="16sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView10" android:textColor="#000000"/>
    <Button
            android:text="REFRESH"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/refreshButton" app:layout_constraintStart_toStartOf="@+id/guideline1"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline2"
            android:layout_marginEnd="8dp"
            android:textSize="20sp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline8" android:textColor="#000000"/>
    <TextView
            android:text="PLUGGED:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" app:layout_constraintStart_toStartOf="@+id/guideline1"
            app:layout_constraintEnd_toStartOf="@+id/guideline7"
            app:layout_constraintTop_toBottomOf="@+id/textView10" android:textAlignment="viewStart"
            android:textSize="16sp" android:textColor="#000000"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBatteryPlugged" app:layout_constraintStart_toStartOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@+id/textViewBatteryStatus" android:textAlignment="viewStart"
            android:textColor="#000000"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline8"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.9"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline9"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.3"/>
</androidx.constraintlayout.widget.ConstraintLayout>